    <style>
        /*timeline history*/
        .timeline-area {
            margin: 10px auto;
            width: 100%;
            border-left: 2px solid #ccc;
            padding: 0 20px 0 30px;
        }

        .timeline-item {
            background-color: #fff;
            padding: 5px 15px;
            font-size: 14px;
            border: 1px solid #ccc;
            line-height: 1.7;
            position: relative;
            margin-bottom: 20px;
        }

            .timeline-item h3, h4 {
                margin: 0;
            }

            /*dot*/
            .timeline-item:before {
                content: '';
                display: block;
                width: 15px;
                height: 15px;
                border-radius: 50%;
                background-color: #000;
                border: 3px solid #ddd;
                position: absolute;
                top: 10px;
                left: -40px;
            }

            .timeline-item:after {
                content: '';
                width: 0;
                height: 0;
                border-style: solid;
                border-color: transparent #ddd transparent transparent;
                border-width: 8px;
                position: absolute;
                left: -17px;
                top: 10px;
            }
    </style>

</head>

